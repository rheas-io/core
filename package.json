{
  "name": "@rheas/core",
  "version": "0.0.1",
  "description": "Rheas framework's core server module",
  "main": "index.js",
  "keywords": [
    "rheas",
    "express",
    "framework",
    "nodejs",
    "laravel",
    "server",
    "library"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "copypackage": "cp -rf package.json build",
    "build": "tsc && npm run copypackage",
    "copypackage_dev": "cp -rf package.json build_dev",
    "build-dev": "tsc --outDir ./build_dev && npm run copypackage_dev"
  },
  "dependencies": {
    "accepts": "^1.3.7",
    "formidable": "^1.2.2",
    "mime-types": "^2.1.27"
  },
  "devDependencies": {
    "@rheas/container": "^0.0.1",
    "@rheas/contracts": "^0.0.1",
    "@rheas/services": "^0.0.1",
    "@rheas/routing": "^0.0.1",
    "@rheas/support": "^0.0.1",
    "@rheas/errors": "^0.0.1",
    "@types/accepts": "^1.3.5",
    "@types/formidable": "^1.0.31",
    "@types/jest": "^25.2.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^13.13.2",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "author": "Kalesh Kaladharan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rheas-io/core.git"
  },
